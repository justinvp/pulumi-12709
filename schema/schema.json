{
    "name": "awsconf",
    "language": {
        "go": {
            "generateResourceContainerTypes": true,
            "importBasePath": "github.com/t0yv0/pulumi-12709/sdk/go/awsconf",
            "liftSingleValueMethodReturns": true
        },
        "nodejs": {
            "dependencies": {
                "@pulumi/aws": "5.31.0"
            }
        }
    },
    "config": {},
    "provider": {},
    "resources": {
        "awsconf:index:Configurer": {
            "isComponent": true,
            "methods": {
                "configureAwsProvider": "awsconf:index:Configurer/awsMethod"
            }
        }
    },
    "functions": {
        "awsconf:index:Configurer/awsMethod": {
            "inputs": {
                "properties": {
                    "__self__": {
                        "$ref": "#/resources/awsconf:index:Configurer"
                    },
                    "mode": {
                        "type": "string"
                    },
                    "profile": {
                        "type": "string"
                    },
                    "region": {
                        "type": "string"
                    }
                },
                "required": [
                    "__self__",
                    "region",
                    "profile"
                ]
            },
            "outputs": {
                "properties": {
                    "awsProvider": {
                        "$ref": "/aws/v5.31.0/schema.json#/provider"
                    },
                    "someString": {
                        "type": "string"
                    }
                },
                "required": [
                    "awsProvider"
                ]
            }
        }
    }
}